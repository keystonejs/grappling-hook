    "bluebird": "^3.5.2",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "grunt": "^1.0.3",
    "grunt-gh-pages": "^3.1.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
